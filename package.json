{
  "name": "@jenssimon/stylelint-config",
  "version": "0.0.0-development",
  "description": "A collection of shareable Stylelint configurations",
  "main": "index.js",
  "author": "Jens Simon <https://github.com/jenssimon>",
  "keywords": [
    "stylelint",
    "stylelintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenssimon/stylelint-config"
  },
  "license": "MIT",
  "files": [
    "/index.js",
    "/base.js",
    "/base-order.js",
    "/order.js",
    "/sfra.js",
    "/lib"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.mjs,.cjs ./",
    "commit": "cz",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^11.0.0"
  },
  "peerDependencies": {
    "stylelint": ">=14.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@jenssimon/eslint-config-base": "^7.3.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "pinst": "^3.0.0",
    "semantic-release": "^22.0.5",
    "stylelint": "^15.11.0"
  },
  "eslintConfig": {
    "extends": [
      "@jenssimon/base"
    ],
    "rules": {
      "unicorn/prefer-module": "off"
    }
  },
  "packageManager": "yarn@4.0.1",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": "eslint"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
