{
  "name": "@jenssimon/stylelint-config",
  "version": "0.0.0-development",
  "description": "A collection of shareable Stylelint configurations",
  "main": "index.js",
  "author": "Jens Simon <https://github.com/jenssimon>",
  "keywords": [
    "stylelint",
    "stylelintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenssimon/stylelint-config"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix ./",
    "ci-test": "run-s \"ci-test:*\"",
    "ci-test:lint": "eslint ./",
    "commit": "cz",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-scss": "^4.3.0"
  },
  "peerDependencies": {
    "stylelint": ">=14.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@jenssimon/eslint-config-base": "6.0.2",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.22.0",
    "husky": "8.0.1",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "semantic-release": "19.0.3",
    "stylelint": "14.10.0"
  },
  "eslintConfig": {
    "extends": [
      "@jenssimon/base"
    ],
    "rules": {
      "unicorn/prefer-module": "off"
    }
  },
  "packageManager": "yarn@3.2.2",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
